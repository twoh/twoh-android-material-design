<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.web.twoh.coolandroiddesign" >

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".application.TWOHsApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FloatingActionButtonActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".TextInputActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".NavigationViewActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".AppContainerTabActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".TabLayoutActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".CollapsingToolbarActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".RefreshViewActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".RecyclerViewActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".PaletteActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".SwitchCompatActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".RippleEffectActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".RatingBarActivity"
            android:label="@string/app_name"
            />
        <activity android:name=".BottomNavigationViewActivity"
            android:label="@string/app_name"
            />
    </application>

</manifest>
